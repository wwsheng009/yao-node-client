{
  "name": "yao-node-client",
  "version": "1.0.0",
  "description": "create yao script with typescript",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r tsconfig-paths/register src/app/scripts/myscript.ts",
    "run": "pnpm run build  && ts-node -r tsconfig-paths/register src/app/scripts/myscript.ts",
    "build": "rimraf dist && tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "build:esnext": "rimraf dist && tsc --module esnext && tsc-alias",
    "build:yao": "rimraf yao && tsc -p ./tsconfig-yao.json && tsc-alias -p ./tsconfig-yao.json",
    "fix:export": "ts-node -r tsconfig-paths/register clean_up.ts",
    "esbuild:comp": "node build.mjs -i dist/app/scripts/rollup/index.js -o dist/app/scripts/rollup/index.dist.js",
    "esbuild:proxy-client": "esbuild yao/yao-node-proxy-client/index.js --bundle --platform=node --outfile=yao/yao-node-proxy-client/bound.index.js",
    "test": "jest",
    "rollup:comp": "rollup -c rollup.config.mjs -i dist/app/scripts/rollup/index.js -o dist/app/scripts/rollup/index.bound.js",
    "rollup:yao": "rollup -c rollup.config.mjs",
    "rollup:build": "pnpm run build:esnext  && rollup -c rollup.config.mjs"
  },
  "keywords": [
    "yao-client",
    "yao"
  ],
  "author": "vincentwwsheng@gmail.com",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "mime": "^3.0.0",
    "sync-fetch": "^0.4.2",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.14.0",
    "@types/sync-fetch": "^0.4.0",
    "@types/ws": "^8.5.4",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.8",
    "glob": "^8.1.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}