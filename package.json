{
  "name": "yao-client",
  "version": "1.0.0",
  "description": "create yao script with typescript",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/scripts/myscript.ts",
    "myscript": "pnpm run build  && ts-node src/scripts/myscript.ts",
    "build": "tsc",
    "run": "node dist/index.js",
    "build_lib": "tsc --module esnext",
    "esbuild": "node build.js"
  },
  "keywords": [
    "yao-client",
    "yao"
  ],
  "author": "vincentwwsheng@gmail.com",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "sync-fetch": "^0.4.2",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/sync-fetch": "^0.4.0",
    "@types/ws": "^8.5.4",
    "esbuild": "^0.17.8",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}