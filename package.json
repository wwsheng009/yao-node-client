{
  "name": "yao-node-client",
  "version": "1.0.0",
  "description": "create yao script with typescript",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' -r tsconfig-paths/register src/app/scripts/myscript.ts",
    "run": "pnpm run build  && ts-node -r tsconfig-paths/register src/app/scripts/myscript.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "build:lib": "tsc --module esnext && tsc-alias",
    "build:yao": "rm -rf yao && tsc -p ./tsconfig-yao.json && tsc-alias && pnpm run clean_up",
    "clean:up": "ts-node -r tsconfig-paths/register src/tools/clean_up.ts",
    "esbuild": "node build.js"
  },
  "keywords": [
    "yao-client",
    "yao"
  ],
  "author": "vincentwwsheng@gmail.com",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "sync-fetch": "^0.4.2",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "@types/sync-fetch": "^0.4.0",
    "@types/ws": "^8.5.4",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.8",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}